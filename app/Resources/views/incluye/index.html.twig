{% extends 'base.html.twig' %}

{% block body %}
    <h1>Incluye list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Valor</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for incluye in incluyes %}
            <tr>
                <td><a href="{{ path('incluye_show', { 'id': incluye.id }) }}">{{ incluye.id }}</a></td>
                <td>{{ incluye.nombre }}</td>
                <td>{{ incluye.valor }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('incluye_show', { 'id': incluye.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('incluye_edit', { 'id': incluye.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('incluye_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
